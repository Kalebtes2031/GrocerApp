{
  "expo": {
    "name": "Yason",
    "slug": "MyFreshApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/yasonlogowhite1.png",
    "scheme": "myfreshapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/yasonlogowhite1.png",
        "backgroundColor": "#001A6E"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.abeniexpo.MyFreshApp",
      "googleServicesFile": "./google-services.json",
       "notification": {
        "icon": "./assets/images/yasonlogowhite1.png",
        "color": "#001A6E",
        "sound": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/yasonlogowhite1.png"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
